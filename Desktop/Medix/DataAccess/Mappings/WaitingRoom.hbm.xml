<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Model" assembly="Model">

  <class name="WaitingRoom" table="waiting_room">
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="Doctor">
      <column name="id_doctor" />
    </property>
    <property name="Nurse">
      <column name="id_nurse" />
    </property>
    <!-- u examination u bazi dodan waiting_room_id tako da mozemo povezat s waiting_room, ili?-->
    <set name = "Examinations" table = "examination_waiting_room">
      <key column = "waiting_room_id"/>
      <one-to-many class = "MedicalExamination"/>
    </set>
  </class>
</hibernate-mapping>