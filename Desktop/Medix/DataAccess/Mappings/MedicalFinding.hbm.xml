<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Model" assembly="Model">

  <class name="MedicalExamination" table="examination">
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="Date">
      <column name="medical_finding_date" />
    </property>
    <many-to-one name="MedicalDiagnosis" class="MedicalDiagnosis" column="medical_diagnosis_id" />
    <many-to-one name="Doctor" class="Doctor" column="doctor_id" />
    <many-to-one name="Patient" class="Patient" column="patient_id" />
    <one-to-one name="_findingDesc" class="MedicalFindingDescription" column="description_id" />
  </class>
</hibernate-mapping>