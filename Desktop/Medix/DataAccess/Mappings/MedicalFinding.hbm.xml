<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Model" assembly="Model">

  <class name="MedicalFinding" table="finding">
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="Date">
      <column name="medical_finding_date" />
    </property>
<!--    <many-to-one name="MedicalDiagnosis" class="MedicalDiagnosis" column="medical_diagnosis_id" not-null="true"/> -->
    <many-to-one name="Doctor" class="Doctor" column="doctor_id" not-null="true" />
    <many-to-one name="Patient" class="Patient" column="patient_id" not-null="true" />
    <many-to-one name="_findingDesc" class="MedicalFindingDescription" column="description_id" not-null="true"/>
  </class>
</hibernate-mapping>